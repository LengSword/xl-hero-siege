		endif
	endif
	if( (GetTriggerUnit() == udg_Heroes[( (1 + GetPlayerId(GetOwningPlayer(GetTriggerUnit())) ) + 8)]) ) then // INLINED!!
		return
	endif
	call DisplayTimedTextToPlayer(GetOwningPlayer(GetTriggerUnit()), 0, 0, 10., "您的英雄达到 |c0061FFFF20级|c00FFFFFF 并得到一种新的技能")
endfunction

